class Solution {
    public int divide(int dividend, int divisor) {
        int max_value = Integer.MAX_VALUE;
        int min_value = Integer.MIN_VALUE;        
        
        int output = dividend/divisor;
        
        if(output <= min_value && dividend<0 && divisor < 0){
            output = max_value;
        }
        
        return output;
    }
}
