class Solution {
    public String restoreString(String s, int[] indices) {
        char[] outputArray = new char[s.length()];
        
        for(int i=0;i<s.length();i++){
            int temp = indices[i];
            outputArray[temp] = s.charAt(i);
            
            System.out.println("Index" + i + ":" + outputArray[temp]);
        }
        
        String result = String. valueOf(outputArray);
        
        return result;
        
        
    }
}
